<Window x:Class="Winleafs.Wpf.Views.Layout.PercentageProfileWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Winleafs.Wpf.Views.Layout"
        mc:Ignorable="d"
        Title="{x:Static local:Resources.PercentageProfileWindow}" Height="828" Width="1000" ResizeMode="CanMinimize">
    <Grid Background="{StaticResource NanoleafBlack}">
        <local:LayoutDisplayUserControl x:Name="LayoutDisplay" Width="800" Height="800" VerticalAlignment="Top" HorizontalAlignment="Right"/>
        <StackPanel x:Name="StepList" HorizontalAlignment="Left" Width="190" Height="740" VerticalAlignment="Bottom"/>
        <Button HorizontalAlignment="Left" VerticalAlignment="Top" Width="35" Height="35" Background="{x:Null}" BorderBrush="{x:Null}" Click="Plus_Click">
            <Image Source="../../Icons/appbar.add.png"></Image>
        </Button>
    </Grid>
</Window>
