<Window x:Class="Winleafs.Wpf.Views.Options.ApiUsedWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Winleafs.Wpf.Views.Options"
        mc:Ignorable="d"
        Title="Winleafs APIs used" Height="250" Width="569.547">
    <Grid Background="{StaticResource NanoleafBlack}">
        <TextBlock>
        <Hyperlink NavigateUri="https://sunrise-sunset.org/api">
            <Label Content="{x:Static local:Resources.SunsetApi}" HorizontalAlignment="Left" Margin="10,50,0,0" VerticalAlignment="Top" FontSize="20" Foreground="White"/>
        </Hyperlink>
        </TextBlock>
        <TextBlock>
            <Hyperlink NavigateUri="https://sunrise-sunset.org/api">
                <Label Content="{x:Static local:Resources.IpApi}" HorizontalAlignment="Left" Margin="10,100,0,0" VerticalAlignment="Top" FontSize="20" Foreground="White"/>
            </Hyperlink>
        </TextBlock>
        <TextBlock>
            <Hyperlink NavigateUri="https://sunrise-sunset.org/api">
                <Label Content="{x:Static local:Resources.LocationApi}" HorizontalAlignment="Left" Margin="10,150,0,0" VerticalAlignment="Top" FontSize="20" Foreground="White"/>
            </Hyperlink>
        </TextBlock>
    </Grid>
</Window>